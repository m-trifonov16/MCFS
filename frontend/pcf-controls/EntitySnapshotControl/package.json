{
    "name": "@fsi-pcf/entity-snapshot-pcf",
    "version": "1.0.0",
    "description": "Project containing your PowerApps Component Framework (PCF) control.",
    "scripts": {
        "build": "npm run build-translations && pcf-scripts build",
        "clean": "pcf-scripts clean",
        "rebuild": "pcf-scripts rebuild",
        "start": "npm run build-translations --isDev && pcf-scripts start watch",
        "build-translations": "node ../../core-components/scripts/translateHelper.js CustomerSnapshotControl",
        "build-libs": "npm run build-frontend && npm run build-common-pcf"
    },
    "dependencies": {
        "@fsi/pcf-common": "1.0.0",
        "@types/node": "^10.17.49",
        "@types/powerapps-component-framework": "^1.2.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "devDependencies": {
        "pcf-scripts": "^1.19.4",
        "pcf-start": "^1.19.4",
        "typescript": "^4.1.5"
    }
}