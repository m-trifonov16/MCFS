    "refreshTypes": "pcf-scripts refreshTypes"
  },
  "dependencies": {
    "@fsi/queue": "1.0.0",
    "@fsi/pcf-common": "1.0.0",
    "@fsi/core-components": "1.0.0",
    "@fsi-pcf/onboarding-application-common": "1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@types/node": "^10.17.49",
    "@types/powerapps-component-framework": "^1.2.0",
    "pcf-scripts": "^1.19.4",
    "pcf-start": "^1.19.4",
    "typescript": "^4.1.5"
  }
}
