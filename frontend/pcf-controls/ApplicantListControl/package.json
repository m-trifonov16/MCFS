{
    "name": "@fsi-pcf/applicant-list-pcf",
    "version": "1.0.0",
    "description": "Project containing your PowerApps Component Framework (PCF) control.",
    "scripts": {
        "build": "npm run build-translations && pcf-scripts build",
        "clean": "pcf-scripts clean",
        "rebuild": "pcf-scripts rebuild",
        "start": "npm run build-translations --isDev && pcf-scripts start watch",
        "build-translations": "node ../../scripts/localization/create-resx.js onboarding-application=OnboardingApplicationTasksControl onboarding-application=ApplicantListControl",
        "refreshTypes": "pcf-scripts refreshTypes"
    },
    "dependencies": {
        "@fsi/pcf-common": "1.0.0",
        "@fsi/core-components": "1.0.0",
        "@fsi-pcf/oa-tasks-common": "1.0.0",
        "@fsi/onboarding-application": "1.0.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "devDependencies": {
        "@types/node": "^10.17.49",
        "@types/powerapps-component-framework": "^1.2.0",
        "pcf-scripts": "^1.19.4",
        "pcf-start": "^1.19.4",
        "typescript": "^4.1.5"
    }
}
